{
  "name": "pubkeyapp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:web && yarn prisma generate && yarn build:api",
    "build:gateway": "yarn build:render && nx build gateway --prod",
    "build:api": "nx build api --prod",
    "build:gum-sdk": "genql --endpoint 'https://aware-earwig-49.hasura.app/v1/graphql' --output ./packages/gum-sdk/src/generated && nx format:write --files ./packages/gum-sdk/src/generated",
    "build:render": "nx build render --prod",
    "build:sdk": "yarn openapi-generator-cli generate -i ./api-swagger.json -g typescript-axios -o packages/sdk/src/generated --additional-properties=withInterfaces=true && nx format:write --uncommitted",
    "build:web": "nx build web --prod",
    "build:web-sdk": "graphql-codegen --config libs/web/util/sdk/src/codegen.yml",
    "dev:gateway": "nx serve gateway",
    "dev:api": "nx serve api",
    "dev:render": "nx serve render",
    "dev:sdk": "nodemon --watch ./api-swagger.json -x 'yarn build:sdk'",
    "dev:services": "docker compose up",
    "dev:web": "nx serve web",
    "dev:web-sdk": "yarn build:web-sdk --watch",
    "docker:build:api": "docker build . -t ghcr.io/pubkeyapp/pubkeyapp-api",
    "docker:build:gateway": "docker build . -f ./Dockerfile.gateway -t ghcr.io/pubkeyapp/pubkeyapp-gateway",
    "docker:push:api": "docker push ghcr.io/pubkeyapp/pubkeyapp-api",
    "docker:push:gateway": "docker push ghcr.io/pubkeyapp/pubkeyapp-gateway",
    "docker:run:api": "docker run -it -p 8000:3000 ghcr.io/pubkeyapp/pubkeyapp-api",
    "docker:run:gateway": "docker run -it -p 3080:3080 -e PUBKEY_ENDPOINT=https://pubkey.ngrok.io ghcr.io/pubkeyapp/pubkeyapp-gateway",
    "ngrok": "ngrok http --log-level debug --region=us --hostname=pubkey.ngrok.io 3040",
    "start": "yarn prisma db push && node dist/apps/api/main.js",
    "start:gateway": "node dist/apps/gateway/main.js"
  },
